{
  "name": "eslint-config-pro",
  "version": "3.0.12",
  "description": "Almost all ESLint v5 rules are proven in practice for professional developers",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": ">=9.3.0",
    "pnpm": "^7.1.6"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NikolayMakhonin/eslint-config-pro.git"
  },
  "keywords": [
    "JavaScript Style",
    "check",
    "checker",
    "code",
    "code checker",
    "code linter",
    "code standards",
    "code style",
    "enforce",
    "eslint",
    "eslintconfig",
    "hint",
    "jscs",
    "jshint",
    "lint",
    "style",
    "style checker",
    "style linter",
    "verify"
  ],
  "author": {
    "name": "Nikolay Makhonin",
    "url": "https://github.com/NikolayMakhonin/"
  },
  "license": "Unlimited Free",
  "bugs": {
    "url": "https://github.com/NikolayMakhonin/eslint-config-pro/issues"
  },
  "homepage": "https://github.com/NikolayMakhonin/eslint-config-pro",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "prepublishOnly": "pnpm run audit && pnpm build && pnpm run lint && pnpm run test:mocha:ci",
    "audit": "pnpm audit --prod",
    "lint": "eslint ./**/*.{js,cjs,mjs,ts,tsx,svelte,html}",
    "lint:wizard": "eslint-nibble --cache --multi ./**/*.{js,cjs,mjs,ts,tsx,svelte,html}",
    "build:js": "rollup -c && cpy \"**/assets/**\" \"../dist/\" --parents --cwd=src",
    "build:types": "tsc --outDir dist --declaration",
    "build": "rimraf dist && pnpm run build:js && pnpm run build:types",
    "test:mocha": "mocha ./src/**/*.test.*",
    "test:mocha:watch": "mocha --watch ./src/**/*.test.*",
    "test:mocha:ci": "mocha ./{src,dist}/**/*.test.*",
    "analysis": "ts-node tools/analysis.ts"
  },
  "devDependencies": {
    "@flemist/test-variants": "^0.0.1",
    "@rollup/plugin-alias": "^3.1.9",
    "@rollup/plugin-commonjs": "^22.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@rollup/plugin-replace": "^4.0.0",
    "@rollup/plugin-typescript": "^8.3.3",
    "@types/assert": "^1.5.6",
    "@types/eslint": "^8.4.3",
    "@types/fs-extra": "^9.0.13",
    "@types/lodash": "^4.14.182",
    "@types/mocha": "^9.1.1",
    "@types/node": "^18.0.0",
    "cpy-cli": "^4.1.0",
    "eslint": "^8.17.0",
    "eslint-plugin-html": "^6.2.0",
    "eslint-plugin-svelte3": ">=4.0.0",
    "fs-extra": "^10.1.0",
    "globby": "^11.1.0",
    "json5": "^2.2.1",
    "lodash": "^4.17.21",
    "mocha": "^9.2.2",
    "multimatch": "^6.0.0",
    "nodemon": "^2.0.16",
    "rimraf": "^3.0.2",
    "rollup": "^2.75.6",
    "rollup-plugin-delete": "^2.0.0",
    "rollup-plugin-multi-input": "^1.3.1",
    "svelte": "^3.48.0",
    "ts-node": "^10.8.1",
    "tsconfig-paths": "^4.0.0",
    "typescript": "^4.7.3"
  },
  "peerDependencies": {
    "eslint": ">=7.4.0",
    "eslint-plugin-html": ">=6.2.0",
    "eslint-plugin-svelte3": ">=4.0.0",
    "typescript": ">=3.3.1"
  },
  "peerDependenciesMeta": {
    "eslint-plugin-html": {
      "optional": true
    },
    "eslint-plugin-svelte3": {
      "optional": true
    }
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^5.28.0",
    "@typescript-eslint/parser": "^5.28.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-node": "^11.1.0",
    "tslib": "^2.4.0"
  }
}
